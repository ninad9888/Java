import java.util.Scanner;

public class A{
    public static void main(String[] args){
        Scanner scanner=new Scanner(System.in);
        String s=scanner.nextLine();
        int v=1;
        int l=s.length();
        for(int i=0;i<l;i++){
            if(" ".indexOf(s.charAt(i))!=-1){
                v++;
                
            }
            
        }
        System.out.println("Words : "+v);
    }
    
}
