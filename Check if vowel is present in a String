import java.util.Scanner;
class Hello{
    public static void main(String[] args){
    Scanner scanner=new Scanner(System.in);
    System.out.println("Enter sentence :");
    String s=scanner.nextLine();
    boolean b;
    if(s.matches(".*[aeiouAEIOU].*")){
        b=true;
    }else{
        b=false;
    }
    System.out.println(b);
}}
